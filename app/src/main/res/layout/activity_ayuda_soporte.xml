<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_ayuda_soporte"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="nupa.drafthatch.AyudaSoporteActivity">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbarNuevoHatch"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:background="?attr/colorPrimary"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/support_agent"
        android:id="@+id/imgSupporrt"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/txtSupporrtLine1"
        android:layout_marginStart="34dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="12dp"
        app:layout_constraintLeft_toLeftOf="@+id/txtSupporrtLine1"
        android:layout_marginLeft="34dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/toolbarNuevoHatch" />

    <TextView
        android:text="Si tenÃ©s alguna duda contactanos y"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtSupporrtLine1"
        android:textSize="14sp"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/txtSupportLine2"
        android:layout_marginStart="23dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/txtSupportLine2"
        android:layout_marginLeft="23dp" />

    <TextView
        android:text="nuestro equipo te respondera a la brevedad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtSupportLine2"
        android:textSize="14sp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="208dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:text="support@nupa.com.ar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtSupportEmail"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="36dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgEmailIcon"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="@+id/imgEmailIcon"
        app:layout_constraintLeft_toRightOf="@+id/imgEmailIcon"
        android:layout_marginLeft="36dp" />

    <TextView
        android:text="@string/urltext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtNupaURl"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/txtSupportEmail"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/txtSupportEmail" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/mail_icon"
        android:id="@+id/imgEmailIcon"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/imageView18"
        android:layout_marginStart="16dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/web_icon"
        android:id="@+id/imageView18"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/imgEmailIcon"
        android:layout_marginTop="105dp"
        app:layout_constraintTop_toBottomOf="@+id/txtSupporrtLine1"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="104dp"
        app:layout_constraintLeft_toLeftOf="@+id/imgEmailIcon" />


</android.support.constraint.ConstraintLayout>
